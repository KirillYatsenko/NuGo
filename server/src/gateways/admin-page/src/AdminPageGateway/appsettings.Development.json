{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "GatewayIP": "http://*:5000" ,

  "ReRoutes": [
      {
          "DownstreamPathTemplate": "/api/events/{everything}",
          "DownstreamScheme": "http",
          "DownstreamHostAndPorts": [
                  {
                      "Host": "localhost",
                      "Port": 5010
                  }
              ],
          "UpstreamPathTemplate": "/api/events/{everything}",
          "UpstreamHttpMethod": [ "Get" ]
      }
  ],
  "GlobalConfiguration": {
      "BaseUrl": "http://localhost:5000"
  }
}
